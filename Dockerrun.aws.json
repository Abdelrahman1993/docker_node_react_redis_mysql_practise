{
    "AWSEBDockerrunVersion": "1",
    "containerDefinitions":[
        {
            "name":"client",
            "image": "abdelrahman1993/multi-client",
            "hostname": "client",
            "essential":false,
            "memory": 128
        },
        {
            "name":"apiserver",
            "image": "abdelrahman1993/multi-server",
            "hostname": "apiserver",
            "essential":false,
            "memory": 128
        },
        {
            "name":"worker",
            "image": "abdelrahman1993/multi-worker",
            "hostname": "worker",
            "essential":false,
            "memory": 128
        },
        {
            "name":"nginx",
            "image": "abdelrahman1993/multi-nginx",
            "hostname": "nginx",
            "essential":true,
            "portMappings":[
                {
                "hostPort": 80,
                "containerPort": 80
                }
            ],
            "links": ["client", "server"],
            "memory": 128
        }
    ]
    
}