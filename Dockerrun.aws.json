{
    "AWSEBDockerrunVersion": "1",
    "ContainerDefinitions":[
        {
            "Name":"client",
            "Image": "abdelrahman1993/multi-client",
            "HostName": "client",
            "Essential":false,
            "Memory": 128,
            "Ports": [
                {
                  "ContainerPort": "3000"
                }
            ]
        },
        {
            "Name":"apiserver",
            "Image": "abdelrahman1993/multi-server",
            "HostName": "apiserver",
            "Essential":false,
            "Memory": 128,
            "Ports": [
                {
                  "ContainerPort": "5000"
                }
            ]
        },
        {
            "Name":"worker",
            "Image": "abdelrahman1993/multi-worker",
            "HostName": "worker",
            "Essential":false,
            "Memory": 128,
            "Ports": [
                {
                  "ContainerPort": "1234"
                }
            ]
        },
        {
            "Name":"nginx",
            "Image": "abdelrahman1993/multi-nginx",
            "HostName": "nginx",
            "Essential":true,
            "Ports":[
                {
                "HostPort": "80",
                "ContainerPort": "80"
                }
            ],
            "Links": ["client", "server"],
            "Memory": 128
        }
    ]
    
}