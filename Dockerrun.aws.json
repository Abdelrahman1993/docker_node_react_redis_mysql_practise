{
    "AWSEBDockerrunVersion": "1",
    "containerDefinitions":[
        {
            "name":"client",
            "image": "abdelrahman1993/multi-client",
            "hostname": "client",
            "essential":false,
            "memory": 128,
            "Ports": [
                {
                  "ContainerPort": "3000"
                }
            ]
        },
        {
            "name":"apiserver",
            "image": "abdelrahman1993/multi-server",
            "hostname": "apiserver",
            "essential":false,
            "memory": 128,
            "Ports": [
                {
                  "ContainerPort": "5000"
                }
            ]
        },
        {
            "name":"worker",
            "image": "abdelrahman1993/multi-worker",
            "hostname": "worker",
            "essential":false,
            "memory": 128,
            "Ports": [
                {
                  "ContainerPort": "1234"
                }
            ]
        },
        {
            "name":"nginx",
            "image": "abdelrahman1993/multi-nginx",
            "hostname": "nginx",
            "essential":true,
            "Ports":[
                {
                "HostPort": 80,
                "ContainerPort": 80
                }
            ],
            "links": ["client", "server"],
            "memory": 128
        }
    ]
    
}